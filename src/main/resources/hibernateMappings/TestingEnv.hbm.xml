<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ged.daedaluswin.crmserver.db.pojos.TestingEnv" table="TESTING_ENV" schema="dbo" catalog="CRM_UAT">
        <id name="id">
            <column name="ID" sql-type="int identity" not-null="true"/>
        </id>
        <property name="name">
            <column name="NAME" sql-type="varchar" length="30"/>
        </property>
        <property name="address">
            <column name="ADDRESS" sql-type="varchar" length="30"/>
        </property>
        <property name="phone">
            <column name="PHONE" sql-type="varchar" length="12"/>
        </property>
    </class>
</hibernate-mapping>