<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ged.daedaluswin.crmserver.db.pojos.ContactEmails" table="ContactEmails" schema="crm"
           catalog="CRM_UAT">
        <id name="id">
            <column name="ID" sql-type="int identity" not-null="true"/>
        </id>
        <property name="email">
            <column name="Email" sql-type="nvarchar" length="100"/>
        </property>
    </class>
</hibernate-mapping>